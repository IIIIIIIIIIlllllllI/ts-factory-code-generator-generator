{
  "name": "ts-factory-code-generator-generator",
  "version": "0.5.3",
  "description": "Generates code that generates TypeScript Compiler API factory code from an AST.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx ttsc",
    "dopublish": "yarn build && echo \"Run: npm publish --otp\"",
    "test": "cross-env TS_NODE_COMPILER=\"ttypescript\" TS_NODE_TRANSPILE_ONLY=\"true\" mocha",
    "test:debug": "yarn run test --inspect-brk",
    "format": "dprint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/ts-factory-code-generator-generator.git"
  },
  "keywords": [
    "typescript",
    "compiler",
    "API",
    "factory",
    "code",
    "generator"
  ],
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/ts-factory-code-generator-generator/issues"
  },
  "homepage": "https://github.com/dsherret/ts-factory-code-generator-generator#readme",
  "dependencies": {
    "string-similarity": "^4.0.1",
    "ts-morph": "^7.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "@types/string-similarity": "^3.0.0",
    "@types/ts-nameof": "^4.2.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "dprint": "^0.9.0",
    "dprint-plugin-jsonc": "^0.4.0",
    "dprint-plugin-typescript": "^0.15.3",
    "mocha": "^7.1.1",
    "ts-nameof": "^4.2.2",
    "ts-node": "^8.8.1",
    "ttypescript": "^1.5.10",
    "typescript-3.2.1": "npm:typescript@3.2.1",
    "typescript-3.6.3": "npm:typescript@3.6.3",
    "typescript-next": "npm:typescript@next"
  },
  "standard-version": {
    "tagPrefix": ""
  }
}
